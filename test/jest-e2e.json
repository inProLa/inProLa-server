{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@app/plugin-b/(.*)": "<rootDir>/../libs/plugin-b/src/$1",
    "@app/plugin-b": "<rootDir>/../libs/plugin-b/src",
    "@app/frameworks/(.*)": "<rootDir>/../libs/names/frameworks/src/$1",
    "@app/frameworks": "<rootDir>/../libs/names/frameworks/src",
    "@app/frameworks-names/(.*)": "<rootDir>/../libs/frameworks-names/src/$1",
    "@app/frameworks-names": "<rootDir>/../libs/frameworks-names/src",
    "@app/author/(.*)": "<rootDir>/../libs/author/src/$1",
    "@app/author": "<rootDir>/../libs/author/src"
  }
}